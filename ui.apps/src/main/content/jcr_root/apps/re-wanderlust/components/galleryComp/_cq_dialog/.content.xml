<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Tour Banner"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <viewButtonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the view button label."
                                                fieldLabel="View Button Label"
                                                name="./buttonLabel"/>
                                            <viewButtonIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the view button icon."
                                                fieldLabel="View Botton Icon"
                                                name="./buttonIconLabel"/>
                                            <Mute
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="check the check Box for Video Mute."
                                                name="./mute"
                                                text="Check The CheckBox For Video Mute"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <Autoplay
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="check the check box for video autoplay."
                                                name="./autoplay"
                                                text="Check The CheckBox For Video Autoplay"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <limit
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                defaultValue="0"
                                                fieldDescription="Enter the limit of items to be shown in the banner."
                                                fieldLabel="Banner Item Limit"
                                                min="{Long}3"
                                                name="./limit"/>
                                            <operatedBy
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the operated by label."
                                                fieldLabel="Operated By Label"
                                                name="./operatedBy"/>
                                            <reviews
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the reviews label."
                                                fieldLabel="Reviews Label"
                                                name="./reviews"/>
                                            <startIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the star icon."
                                                fieldLabel="Start Icon"
                                                name="./startIconLabel"/>
                                            <defaultImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the default image."
                                                fieldLabel="Default Image"
                                                name="./defaultImageLabel"/>
                                            <redirectionPath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the redirection path."
                                                fieldLabel="Redirection Path"
                                                name="./redirectionPath"/>
                                            <galleryImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the gallery image."
                                                fieldLabel="Gallery Image"
                                                name="./galleryImageIcon"/>
                                        </items>
                                    </columns>
                                </items>
                            </columns>
                        </items>
                    </basic>
                    <error
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Error Label"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <errorLabels
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                                path="re-wanderlust/components/commonFields/errorLabels"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </error>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
