<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Rating And Review"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <rating
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Rating"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ratingTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title of rating and review."
                                                fieldLabel="Rating Title"
                                                name="./ratingTitle"/>
                                            <tripRating
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of trip rating label."
                                                fieldLabel="Trip Rating Label"
                                                name="./tripRating"/>
                                            <bikeRating
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of bike rating label."
                                                fieldLabel="Bike Rating Label"
                                                name="./bikeRating"/>
                                            <submitRating
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of submit rating button label."
                                                fieldLabel="Submit Rating Button Label"
                                                name="./submitRating"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </rating>
                    <msg
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Message"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <greeting
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the greeting text."
                                                fieldLabel="Greeting Text"
                                                name="./greeting"/>
                                            <greetingDesp
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the description for the greeting."
                                                fieldLabel="Greeting Description"
                                                name="./greetingDesp"/>
                                            <reviewbtnTxt
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of review button label."
                                                fieldLabel="Review Button Label"
                                                name="./reviewBtnTxt"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </msg>
                    <review
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Review"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <reviewTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of review textarea title."
                                                fieldLabel="Review Textarea Title"
                                                required="{Boolean}true"
                                                name="./reviewTitle"/>
                                            <placeholderText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the placeholder text for review input box."
                                                fieldLabel="Review Textarea Placeholder Text"
                                                name="./preText"/>
                                            <submitReviewText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of submit review button label."
                                                fieldLabel="Submit Review Button Label"
                                                name="./submitReviewText"/>
                                            <limit
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldDescription="Enter the limit of characters for review."
                                                fieldLabel="Limit Of Characters"
                                                name="./limit"/>
                                            <thankyouPopMsg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of thankyou label."
                                                fieldLabel="Thankyou Label"
                                                name="./thankyouPopMsg"/>
                                            <closeBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of close button label."
                                                fieldLabel="Close Button Label"
                                                name="./closeBtn"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </review>
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <laterBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the text of later link label."
                                                fieldLabel="Later Link label"
                                                name="./laterBtn"/>
                                            <laterBtnUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the redirect path of later link."
                                                fieldLabel="Later Redirect Link"
                                                name="./laterBtnUrl"
                                                rootPath="/content/royal-enfield"/>
                                            <errForLimit
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the error message for limit exceeded."
                                                fieldLabel="Limit Exceeded"
                                                name="./errForLimit"/>
                                            <errForUnfillBox
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the error message for if user click on submit button without fill review."
                                                fieldLabel="Empty Review Inputbox Error Message"
                                                name="./errForUnfillBox"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </basic>
                    <error
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Error Label"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <errorLabels
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                                path="re-wanderlust/components/commonFields/errorLabels"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </error>
                    <hideComponent
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Hide Component"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <hideComponent
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                                path="re-wanderlust/components/commonFields/hideComponent"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </hideComponent>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
